.security-alert {
    display: flex;
    align-items: center;
    padding: 16rpx 20rpx;
    background: #FFF3E0;
    border: 1rpx solid #FFB74D;
    border-radius: 8rpx;
    margin-top: 16rpx;
  }
  
  .alert-icon {
    margin-right: 12rpx;
    font-size: 24rpx;
  }
  
  .alert-text {
    font-size: 24rpx;
    color: #FF9800;
  }